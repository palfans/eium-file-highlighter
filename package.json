{
  "name": "eium-file-highlighter",
  "displayName": "eIUM File Highlighter",
  "description": "Syntax highlighting for eIUM files with .cfg and .config extensions",
  "version": "1.0.1",
  "publisher": "palfansz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/palfans/eium-file-highlighter"
  },
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "eium",
    "syntax",
    "highlight",
    "cfg",
    "config"
  ],
  "galleryBanner": {
    "color": "#C80000",
    "theme": "dark"
  },
  "contributes": {
    "languages": [{
      "id": "eium",
      "aliases": ["eIUM", "eium"],
      "extensions": [".cfg",".config"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "eium",
      "scopeName": "source.eium",
      "path": "./syntaxes/eium.tmLanguage.json"
    }]
  }
}
